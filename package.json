{
  "name": "casino-syndicate",
  "scripts": {
    "start": "concurrently  \"npm run chain:start\" \"npm run contract:watch\" \"npm run app:start\"",
    "chain:start": "npx hardhat node",
    "app:build": "vite build",
    "app:start": "vite",
    "contract": "npx hardhat compile && npx hardhat run hardhat.deploy.js --network localhost",
    "contract:watch": "nodemon"
  },
  "dependencies": {
    "@nomicfoundation/hardhat-toolbox": "^5.0.0",
    "@vitejs/plugin-react": "^4.3.4",
    "concurrently": "^9.1.2",
    "dotenv": "^16.4.7",
    "eslint": "^9.21.0",
    "eslint-plugin-react": "^7.37.4",
    "eslint-plugin-react-hooks": "^5.2.0",
    "ethers": "^6.13.5",
    "hardhat": "^2.22.19",
    "nodemon": "^3.1.9",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "vite": "^6.2.0",
    "vite-plugin-eslint": "^1.8.1",
    "web3modal": "^1.9.12"
  }
}
